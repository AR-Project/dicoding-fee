{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "linebreak-style": "off",
        "class-methods-use-this": "off",
        "no-alert": "off",
        "no-console": "off",
        "no-restricted-globals": "off"
    }
}